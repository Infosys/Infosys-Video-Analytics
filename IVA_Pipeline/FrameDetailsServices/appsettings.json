{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "FaceMaskDetectionEntities": "Server=[Server];Database=[Database];User Id=sa;Password=[Password];",
    "FrameDetailStore": "Server=[Server];Database=[Database];User Id=[Id];Password=[Password];"
  },
  "AppSettings": {
    "EnableAllLogs": false,
    "EnablePerformanceLog": false,
    "TenantID": 1,
    "DeviceID": "DeviceId_10",
    "MaxFailCount": 10,
    "MaxThreadOnPool": 10,
    "MinThreadOnPool": 0,
    "ReduceFrameQualityTo": 20,
    "VideoFormatsToUse": [
      ".mp4",
      ".wmv"
    ],
    "OfflineProcessInterval": 10,
    "ServiceBaseUrl": "[ServiceBaseUrl]",
    "FTPCycle": 120,
    "FrameTimeDifferenceIgnoreThreshold": 0.01,
    "ConfigWebApi": "[ConfigWebApi]",
    "FrameDetailsWebApi": "[FrameDetailsWebApi]",
    "EmptyFrameProcessInterval": 60000,
    "MaxEmptyFrameCount": 100,
    "CalculateFrameGrabberFPR": "Yes",
    "FARCheckWaitTime": 60000,
    "CounterInstanceToBeReset": "[DeviceId]",
    "ProcessLoaderTraceFile": "TextOutput.txt"
  }
}
